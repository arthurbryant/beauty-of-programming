#include <stdio.h>

int
sum (int arr[], int n)
{
  int i, j;
  int result = -32768;
  int tmp;
  for (i = 0; i < n; ++i)
    {
      tmp = 0;
      for (j = i; j < n; ++j)
	{
	  tmp += arr[j];
	  if (tmp > result)
	    result = tmp;
	}
    }
return result;
}
int
main (int argc, char *argv[])
{
        int arr[] = {};

  return 0;
}
